<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/zaojiao_bg"
    tools:context=".Early_edu">


    <include
        android:id="@+id/inclu1"
        layout="@layout/power_voice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/music111"
        android:layout_width="190dp"
        android:layout_height="330dp"
        android:layout_marginStart="381dp"
        android:layout_marginTop="119dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qq_music" />



    <ImageView
        android:id="@+id/story"
        android:layout_width="190dp"
        android:layout_height="330dp"
        android:layout_marginStart="574dp"
        android:layout_marginTop="119dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/story" />

    <ImageView
        android:id="@+id/English"
        android:layout_width="190dp"
        android:layout_height="330dp"
        android:layout_marginStart="767dp"
        android:layout_marginTop="119dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/yingyu" />
    <ImageView
        android:id="@+id/music_play"
        android:layout_width="147dp"
        android:layout_height="65dp"
        android:layout_marginStart="402dp"
        android:layout_marginTop="357dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bofang" />

    <ImageView
        android:id="@+id/story_play"
        android:layout_width="147dp"
        android:layout_height="65dp"
        android:layout_marginStart="602dp"
        android:layout_marginTop="357dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bofang" />

    <ImageView
        android:id="@+id/yingyu_play"
        android:layout_width="147dp"
        android:layout_height="65dp"
        android:layout_marginStart="787dp"
        android:layout_marginTop="357dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bofang" />

    <TextView
        android:id="@+id/accelerate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="490dp"
        android:layout_marginTop="0dp"
        android:text="早教"
        android:textColor="#FFFFFF"
        android:textSize="35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/back"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

</androidx.constraintlayout.widget.ConstraintLayout>