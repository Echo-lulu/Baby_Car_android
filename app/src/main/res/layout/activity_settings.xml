<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setting">
    <include
        android:id="@+id/inclu1"
        layout="@layout/car_state"
        android:layout_width="1068dp"
        android:layout_height="600dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        />

    <ImageView
        android:id="@+id/wifi"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:layout_marginStart="318dp"
        android:layout_marginTop="146dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/wifi" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="WIFI设置"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        android:layout_marginStart="321dp"
        android:layout_marginTop="277dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/bluetooth"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:layout_marginStart="477dp"
        android:layout_marginTop="146dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bluetooth" />

    <TextView
        android:id="@+id/textView191"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="蓝牙设置"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        android:layout_marginStart="482dp"
        android:layout_marginTop="277dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/fly"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:layout_marginStart="636dp"
        android:layout_marginTop="146dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fly" />

    <TextView
        android:id="@+id/textView191s"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="飞行模式"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        android:layout_marginStart="641dp"
        android:layout_marginTop="277dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/back"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

</androidx.constraintlayout.widget.ConstraintLayout>